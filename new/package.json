    "storybook:build": "storybook build",
    "check": "run-p check:*",
    "check:lint": "eslint . --ext .js,.jsx,.ts,.tsx --max-warnings 0 --report-unused-disable-directives",
    "check:types": "tsc --tsBuildInfoFile .tsbuildinfo --incremental",
    "check:prettier": "prettier --check './{src,test,e2e}/**/*.{js,jsx,ts,tsx,json,scss,css}'",
    "check:test": "vitest run",
    "e2e:headless": "npx playwright test --project \"chromium\" --project \"firefox\"",
    "e2e:ui": "npx playwright test --ui",
    "prettier": "prettier --write './{src,test,e2e}/**/*.{js,jsx,ts,tsx,json,scss,css}'"
