  "scripts": {
    "dev": "run-p dev:*",
    "dev:ui": "vite",
    "dev:pages": "vite --config src/_direktiv-pages/vite.config.mts",
    "build": "run-p build:*",
    "build:check": "pnpm run check",
    "build:compile-ui": "vite --config vite.config.mts build",
    "build:compile-pages": "vite --config src/_direktiv-pages/vite.config.mts build",
    "preview": "run-p preview:*",
    "preview:ui": "vite preview --debug",
    "preview:pages": "vite --config src/_direktiv-pages/vite.config.mts preview --debug",
    "test": "vitest",
    "storybook": "NODE_OPTIONS=--openssl-legacy-provider pnpm exec storybook dev -p 6006",
    "storybook:build": "pnpm exec storybook build",
    "check": "run-p check:*",
    "check:lint": "eslint . --ext .js,.jsx,.ts,.tsx,.mts --max-warnings 0 --report-unused-disable-directives --cache",
    "check:types": "tsc --tsBuildInfoFile .tsbuildinfo --incremental",
    "check:prettier": "prettier --check './{src,test,e2e}/**/*.{js,jsx,ts,tsx,json,scss,css}' --cache",
    "check:deps": "pnpm install --frozen-lockfile --prefer-offline",
    "check:test": "vitest run",
    "check:knip": "knip",
    "fix": "run-p fix:*",
    "fix:prettier": "prettier --write './{src,test,e2e}/**/*.{js,jsx,ts,tsx,json,scss,css}' --cache",
    "fix:knip": "knip --fix --allow-remove-files",
    "e2e:headless": "pnpm exec playwright test --project \"chromium\"",
    "e2e:ui": "pnpm exec playwright test --ui"
  },
  "resolutions": {
    "babel-loader": "8.1.0",
    "react-error-overlay": "6.0.9"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "npm": "9.14.4",
    "node": "20.18.1"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^4.1.1",
    "@faker-js/faker": "^9.2.0",
    "@playwright/test": "^1.51.1",
    "@storybook/addon-docs": "^9.1.10",
